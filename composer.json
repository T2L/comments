{
    "name": "t2l/comments",
    "description": "A tool for preserving comments, e.g. when parsing YAML files.",
    "license": "MIT",
    "authors": [
        {
            "name": "Greg Anderson",
            "email": "greg.1.anderson@greenknowe.org"
        },
        {
            "name": "Roman Paska",
            "email": "roman.paska@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.5.0"
    },
    "require-dev": {
        "dealerdirect/phpcodesniffer-composer-installer": "^0.6.0",
        "phpunit/phpunit": "^8.5 || ~9.0.0",
        "squizlabs/php_codesniffer": "^3.5",
        "symfony/yaml": "^3.4 || ^4.4 || ^5"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    },
    "autoload": {
        "psr-4": {
            "Consolidation\\Comments\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Consolidation\\Comments\\": "tests"
        }
    }
}
